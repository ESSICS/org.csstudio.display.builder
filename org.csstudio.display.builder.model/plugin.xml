<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="widgets" name="Display Builder Widget Model" schema="schema/widgets.exsd"/>
   <extension
         point="org.csstudio.display.builder.model.widgets">
      <widget
            category="GRAPHIC"
            class="org.csstudio.display.builder.model.widgets.LabelWidget"
            description="%LabelWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/label.png"
            name="%LabelWidget_Name"
            type="label">
         <alternates
               type="org.csstudio.opibuilder.widgets.Label">
         </alternates>
      </widget>
      <widget
            category="GRAPHIC"
            class="org.csstudio.display.builder.model.widgets.RectangleWidget"
            description="%RectangleWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/rectangle.png"
            name="%RectangleWidget_Name"
            type="rectangle">
         <alternates
               type="org.csstudio.opibuilder.widgets.Rectangle">
         </alternates>
      </widget>
      <widget
            category="MONITOR"
            class="org.csstudio.display.builder.model.widgets.ImageWidget"
            description="%ImageWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/image.png"
            name="%ImageWidget_Name"
            type="image">
         <alternates
               type="org.csstudio.opibuilder.widgets.intensityGraph">
         </alternates>
      </widget>
      <widget
            category="MONITOR"
            class="org.csstudio.display.builder.model.widgets.LEDWidget"
            description="%LEDWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/led.png"
            name="%LEDWidget_Name"
            type="led">
         <alternates
               type="org.csstudio.opibuilder.widgets.LED">
         </alternates>
      </widget>
      <widget
            category="MONITOR"
            class="org.csstudio.display.builder.model.widgets.ProgressBarWidget"
            description="%ProgressBar_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/progressbar.png"
            name="%ProgressBar_Name"
            type="progressbar">
         <alternates
               type="org.csstudio.opibuilder.widgets.progressbar">
         </alternates>
      </widget>
      <widget
            category="MONITOR"
            class="org.csstudio.display.builder.model.widgets.TextUpdateWidget"
            description="%TextUpdateWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/textupdate.png"
            name="%TextUpdateWidget_Name"
            type="textupdate">
         <alternates
               type="org.csstudio.opibuilder.widgets.TextUpdate">
         </alternates>
      </widget>
      <widget
            category="CONTROL"
            class="org.csstudio.display.builder.model.widgets.ActionButtonWidget"
            description="%ActionButtonWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/action_button.png"
            name="%ActionButtonWidget_Name"
            type="action_button">
         <alternates
               type="org.csstudio.opibuilder.widgets.ActionButton">
         </alternates>
      </widget>
      <widget
            category="CONTROL"
            class="org.csstudio.display.builder.model.widgets.TextEntryWidget"
            description="%TextEntryWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/textentry.png"
            name="%TextEntryWidget_Name"
            type="textentry">
         <alternates
               type="org.csstudio.opibuilder.widgets.TextInput">
         </alternates>
      </widget>
      <widget
            category="PLOT"
            class="org.csstudio.display.builder.model.widgets.XYPlotWidget"
            description="%XYPlotWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/xyplot.png"
            name="%XYPlotWidget_Name"
            type="xyplot">
         <alternates
               type="org.csstudio.opibuilder.widgets.xyGraph">
         </alternates>
      </widget>
      <widget
            category="STRUCTURE"
            class="org.csstudio.display.builder.model.widgets.EmbeddedDisplayWidget"
            description="%EmbeddedDisplayWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/embedded.png"
            name="%EmbeddedDisplayWidget_Name"
            type="embedded">
         <alternates
               type="org.csstudio.opibuilder.widgets.linkingContainer">
         </alternates>
      </widget>
      <widget
            category="STRUCTURE"
            class="org.csstudio.display.builder.model.widgets.GroupWidget"
            description="%GroupWidget_Description"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/group.png"
            name="%GroupWidget_Name"
            type="group">
         <alternates
               type="org.csstudio.opibuilder.widgets.groupingContainer">
         </alternates>
      </widget>
   </extension>

</plugin>
