<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.csstudio.display.builder.rcp.run.RuntimeViewPart"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/display.png"
            id="org.csstudio.display.builder.rcp.run.RuntimeViewPart"
            name="%Runtime"
            restorable="true">
      </view>
      <view
            class="org.csstudio.display.builder.rcp.run.PlaceHolderView"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/display.png"
            id="org.csstudio.display.builder.rcp.run.placeHolder"
            name="%Runtime"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=new.ext">
         <dynamic
               class="org.csstudio.display.builder.rcp.top.TopDisplaysMenu"
               id="org.csstudio.display.builder.rcp.menu.displays">
         </dynamic>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.csstudio.platform.ui.css.applications.display"
            class="org.csstudio.display.builder.rcp.preferences.DisplayPreferencePage"
            id="org.csstudio.display.builder.rcp.preferences.page"
            name="%PreferencePage">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            default="false"
            extensions="bob"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/display.png"
            id="org.csstudio.display.builder.rcp.RuntimeLauncher"
            launcher="org.csstudio.display.builder.rcp.run.RuntimeLauncher"
            name="%RuntimeView">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.csstudio.display.builder.rcp.RuntimePerspectiveFactory"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/display.png"
            id="org.csstudio.display.builder.rcp.runperspective"
            name="%Runtime">
      </perspective>
   </extension>

</plugin>
