<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.csstudio.display.builder.rcp.run.RuntimeViewPart"
            icon="platform:/plugin/org.csstudio.display.builder.model/icons/display.png"
            id="org.csstudio.display.builder.rcp.run.RuntimeViewPart"
            name="%RuntimeView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=new.ext">
         <dynamic
               class="org.csstudio.display.builder.rcp.top.TopDisplaysMenu"
               id="org.csstudio.display.builder.rcp.menu.displays">
         </dynamic>
      </menuContribution>
   </extension>

</plugin>
